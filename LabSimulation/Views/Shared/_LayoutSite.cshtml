                        </li>
                        <li class="nav-item  @Html.IsActive("Contact")">
                            <a href="~/Contact/Index" class="nav-link text-uppercase font-weight-bold">Contact</a>
                        </li>
                    </ul>

                    <ul class="navbar-nav">
                        @if (Request.Cookies["Login"] == null)
                        {
                            <li class="nav-item">
                                <a class="nav-link text-uppercase font-weight-bold " href="~/Users/SignIn">Signin</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-uppercase font-weight-bold " href="~/Users/SignUp">Signup</a>
                            </li>
                        }
                        else
                        {
                            if (Request.Cookies["Login"]["UserType"] == "School")
                            {
                                <li class="nav-item @Html.IsActive("Students")">
                                    <a class="nav-link text-uppercase font-weight-bold " href="~/Students/Index">Students</a>
                                </li>
                            }
                            <li class="nav-item @Html.IsActive("Profile")">
                                <a class="nav-link text-uppercase font-weight-bold " href="~/Profile/Index/@Request.Cookies["Login"]["UserID"]">Profile</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link text-uppercase font-weight-bold " href="~/Home/Logout">Logout</a>
                            </li>
                        }

                    </ul>


                </div>
